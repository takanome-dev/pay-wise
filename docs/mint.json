{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#9563FF",
    "light": "#AE87FF",
    "dark": "#0D001D",
    "background": {
      "dark": "#090014"
    },
    "anchors": {
      "from": "#FF7F57",
      "to": "#9563FF"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:hi@mintlify.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.mintlify.com"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Home",
      "icon": "house-blank",
      "url": "http://localhost:8080"
    },
    {
      "name": "Changelog",
      "icon": "clock-rotate-left",
      "url": "changelog"
    }
  ],
  "api": {
    "baseUrl": "http://localhost:3000/v1",
    "auth": {
      "method": "bearer",
      "name": "Authorization"
    },
    "playground": {
      "mode": "show"
    }
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development", "faq"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/authentication"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoints/get",
        "api-reference/endpoints/create",
        "api-reference/endpoints/update",
        "api-reference/endpoints/delete"
      ]
    },
    {
      "group": "Cards",
      "pages": ["api-reference/endpoints/cards/get"]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/takanome_dev",
    "github": "https://github.com/takanome-dev",
    "linkedin": "https://www.linkedin.com/in/takanome-dev/"
  }
}
